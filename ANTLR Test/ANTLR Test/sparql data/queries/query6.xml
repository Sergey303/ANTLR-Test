<?xml version="1.0" encoding="utf-8"?>
<sparql>
  <prefix>
    <name>rdfs:</name>
    <name>&lt;http://www.w3.org/2000/01/rdf-schema#&gt;</name>
  </prefix>
  <prefix>
    <name>rdf:</name>
    <name>&lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;</name>
  </prefix>
  <prefix>
    <name>bsbm:</name>
    <name>&lt;http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/&gt;</name>
  </prefix>
  <select>
    <variable>?product</variable>
    <variable>?label</variable>
  </select>
  <WHERE>
    <s>
      <var>?product</var>
      <p>
        <PREFIXED_NAME>rdf:type</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm:Product</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <s>
      <var>?product</var>
      <p>
        <PREFIXED_NAME>rdf:type</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm:Product</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <Filter>
      <regex>
        <var>?label</var>
        <RDFLiteral>"^s"</RDFLiteral>
      </regex>
    </Filter>
  </WHERE>
</sparql>