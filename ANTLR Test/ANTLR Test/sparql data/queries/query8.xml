<?xml version="1.0" encoding="utf-8"?>
<sparql>
  <prefix>
    <name>bsbm-inst:</name>
    <name>&lt;http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/&gt;</name>
  </prefix>
  <prefix>
    <name>bsbm:</name>
    <name>&lt;http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/&gt;</name>
  </prefix>
  <prefix>
    <name>dc:</name>
    <name>&lt;http://purl.org/dc/elements/1.1/&gt;</name>
  </prefix>
  <prefix>
    <name>rev:</name>
    <name>&lt;http://purl.org/stuff/rev#&gt;</name>
  </prefix>
  <prefix>
    <name>foaf:</name>
    <name>&lt;http://xmlns.com/foaf/0.1/&gt;</name>
  </prefix>
  <select>
    <variable>?title</variable>
    <variable>?text</variable>
    <variable>?reviewDate</variable>
    <variable>?reviewer</variable>
    <variable>?reviewerName</variable>
    <variable>?rating1</variable>
    <variable>?rating2</variable>
    <variable>?rating3</variable>
    <variable>?rating4</variable>
  </select>
  <WHERE>
    <s>
      <var>?review</var>
      <p>
        <PREFIXED_NAME>bsbm:reviewFor</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm-inst:ProductFeature16962</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <s>
      <var>?review</var>
      <p>
        <PREFIXED_NAME>bsbm:reviewFor</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm-inst:ProductFeature16962</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <Filter>
      <LANGMATCHES>
        <LANG>
          <var>?text</var>
        </LANG>
        <RDFLiteral>"EN"</RDFLiteral>
      </LANGMATCHES>
    </Filter>
    <s>
      <var>?review</var>
      <p>
        <PREFIXED_NAME>bsbm:reviewDate</PREFIXED_NAME>
        <o>
          <var>?reviewDate</var>
        </o>
      </p>
    </s>
    <s>
      <var>?review</var>
      <p>
        <PREFIXED_NAME>bsbm:reviewDate</PREFIXED_NAME>
        <o>
          <var>?reviewDate</var>
        </o>
      </p>
    </s>
    <Optional>
      <s>
        <var>?review</var>
        <p>
          <PREFIXED_NAME>bsbm:rating1</PREFIXED_NAME>
          <o>
            <var>?rating1</var>
          </o>
        </p>
      </s>
    </Optional>
    <Optional>
      <s>
        <var>?review</var>
        <p>
          <PREFIXED_NAME>bsbm:rating2</PREFIXED_NAME>
          <o>
            <var>?rating2</var>
          </o>
        </p>
      </s>
    </Optional>
    <Optional>
      <s>
        <var>?review</var>
        <p>
          <PREFIXED_NAME>bsbm:rating3</PREFIXED_NAME>
          <o>
            <var>?rating3</var>
          </o>
        </p>
      </s>
    </Optional>
    <Optional>
      <s>
        <var>?review</var>
        <p>
          <PREFIXED_NAME>bsbm:rating4</PREFIXED_NAME>
          <o>
            <var>?rating4</var>
          </o>
        </p>
      </s>
    </Optional>
  </WHERE>
  <orderBy>
    <brackettedExpression direction="desc">
      <var>?reviewDate</var>
    </brackettedExpression>
  </orderBy>
  <limit>20</limit>
</sparql>