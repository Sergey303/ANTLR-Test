<?xml version="1.0" encoding="utf-8"?>
<sparql>
  <prefix>
    <name>bsbm-inst:</name>
    <name>&lt;http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/&gt;</name>
  </prefix>
  <prefix>
    <name>bsbm:</name>
    <name>&lt;http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/&gt;</name>
  </prefix>
  <prefix>
    <name>rdfs:</name>
    <name>&lt;http://www.w3.org/2000/01/rdf-schema#&gt;</name>
  </prefix>
  <prefix>
    <name>rdf:</name>
    <name>&lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;</name>
  </prefix>
  <select>
    <variable>?product</variable>
    <variable>?label</variable>
  </select>
  <WHERE>
    <s>
      <var>?product</var>
      <p>
        <PREFIXED_NAME>bsbm:productFeature</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm-inst:ProductFeature1</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <s>
      <var>?product</var>
      <p>
        <PREFIXED_NAME>bsbm:productFeature</PREFIXED_NAME>
        <o>
          <PREFIXED_NAME>bsbm-inst:ProductFeature1</PREFIXED_NAME>
        </o>
      </p>
    </s>
    <Filter>
      <more>
        <var>?p1</var>
        <numericLiteral>1</numericLiteral>
      </more>
    </Filter>
    <s>
      <var>?product</var>
      <p>
        <PREFIXED_NAME>bsbm:productPropertyNumeric3</PREFIXED_NAME>
        <o>
          <var>?p3</var>
        </o>
      </p>
    </s>
    <Filter>
      <les>
        <var>?p3</var>
        <numericLiteral>100000</numericLiteral>
      </les>
    </Filter>
    <Optional>
      <s>
        <var>?product</var>
        <p>
          <PREFIXED_NAME>bsbm:productFeature</PREFIXED_NAME>
          <o>
            <PREFIXED_NAME>bsbm-inst:ProductFeature2</PREFIXED_NAME>
          </o>
        </p>
      </s>
      <s>
        <var>?product</var>
        <p>
          <PREFIXED_NAME>bsbm:productFeature</PREFIXED_NAME>
          <o>
            <PREFIXED_NAME>bsbm-inst:ProductFeature2</PREFIXED_NAME>
          </o>
        </p>
      </s>
    </Optional>
    <Filter>
      <not>
        <BOUND>
          <var>?testVar</var>
        </BOUND>
      </not>
    </Filter>
  </WHERE>
  <orderBy>
    <var>?label</var>
  </orderBy>
  <limit>10</limit>
</sparql>